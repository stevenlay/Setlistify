    "url": "https://github.com/stevenlay/setlistify/issues"
  },
  "homepage": "https://github.com/stevenlay/setlistify#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.13",
    "passport": "^0.4.1",
    "passport-spotify": "^2.0.0",
    "stripe": "^8.132.0"
  },
